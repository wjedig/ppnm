set terminal svg background "white"
set key left
set tics out
set title "Artificial Neural Network fit"
set output "ANN.svg"

f(x) = cos(5*x-1)*exp(-x*x)

plot [-1:1][-2:2] "out.atrain.data" u 1:2 title "Sampled points for training" w points,\
	f(x) with lines title "Target Function",\
	"out.afit.data" u 1:2 title "Fit from neural network" w lines
	
